                    <Dim type="RightEdge" ><UnifiedDim scale="1.0" type="RightEdge" /></Dim>
                    <Dim type="BottomEdge" >
                        <OperatorDim op="Subtract">
                            <UnifiedDim scale="1.0" type="BottomEdge" />
                            <OperatorDim op="Add">
                                <WidgetDim widget="__auto_decbtn__" dimension="Height" />
                                <OperatorDim op="Multiply">
                                    <ImageDim name="EditorLook/VertScrollTop" dimension="Height" />
                                    <AbsoluteDim value="0.5" />
                                </OperatorDim>
                            </OperatorDim>
                        </OperatorDim>
                    </Dim>
                </Area>
                <Image name="EditorLook/VertScrollBarSegment" />
                <VertFormat type="Tiled" />
            </ImageryComponent>
        </ImagerySection>
        <StateImagery name="Enabled">
            <Layer>
                <Section section="main" />
            </Layer>
        </StateImagery>
        <StateImagery name="Disabled">
            <Layer>
                <Section section="main">
                    <Colours topLeft="FF7F7F7F" topRight="FF7F7F7F" bottomLeft="FF7F7F7F" bottomRight="FF7F7F7F" />
                </Section>
            </Layer>
        </StateImagery>
    </WidgetLook>

